# Environment variables for Immigration Policy Tracker with AI Analysis
# Copy this file to .env and update the values with your actual credentials

# Confluent Cloud Kafka Configuration
KAFKA_BOOTSTRAP_SERVERS=your-cluster.region.aws.confluent.cloud:9092
KAFKA_API_KEY=your-api-key
KAFKA_API_SECRET=your-api-secret
KAFKA_TOPIC=immigration.raw_updates

# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_APPLICATION_CREDENTIALS=google-credentials.json

# Vertex AI Configuration
VERTEX_AI_LOCATION=us-central1

# API Server Configuration
PORT=8000